{
  "name": "npm-links",
  "displayName": "npm-links",
  "description": "npm 包相关链接",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.79.1"
  },
  "publisher": "Dolov",
  "repository": "https://github.com/Dolov/vscode-npm-links",
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "npm-links.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "~14.18.53",
    "@types/vscode": "^1.79.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "glob": "^7.2.3",
    "mocha": "^8.4.0",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "vscode-test": "^1.6.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@babel/parser": "^7.22.5",
    "axios": "^0.26.1"
  }
}
